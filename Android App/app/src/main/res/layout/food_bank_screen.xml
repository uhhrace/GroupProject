                    android:layout_height="wrap_content"
                    android:layout_weight="10"
                    android:text="Earthworms" />

                <ToggleButton
                    android:id="@+id/toggleEarthworms"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/food_bank_toggle"
                    android:textOff=""
                    android:textOn="" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_marginTop="5dp"
                android:background="@drawable/rounded_corner_inactive">

                <TextView
                    android:id="@+id/CalciWormsText"
                    android:textColor="@color/buttonDarkBlue"
                    android:fontFamily="@font/rubik_medium"
                    android:paddingHorizontal="25dp"
                    android:textSize="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="10"
                    android:text="Calci Worms" />

                <ToggleButton
                    android:id="@+id/toggleCalciWorms"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/food_bank_toggle"
                    android:textOff=""
                    android:textOn="" />
            </LinearLayout>

        </LinearLayout>
    </ScrollView>

</RelativeLayout>